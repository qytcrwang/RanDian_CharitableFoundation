<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.fire.back.dao.extend.ExtendMsgTbMapper" >
    <resultMap id="msgWithOutContext" type="com.fire.back.entity.MsgTb" >
        <id column="id" property="id" jdbcType="BIGINT" />
        <result column="title" property="title" jdbcType="VARCHAR" />
        <result column="time" property="time" jdbcType="BIGINT" />
        <result column="state" property="state" jdbcType="INTEGER" />
        <result column="if_read" property="ifRead" jdbcType="INTEGER" />
        <result column="msg_type" property="msgType" jdbcType="INTEGER" />
        <result column="is_delete" property="isDelete" jdbcType="INTEGER" />
    </resultMap>
    <select id="getMsgsWithOutContent" resultMap="msgWithOutContext" parameterType="map" >
        select m.id,m.title,m.time,m.state,m.if_read,m.msg_type,m.is_delete from msg_tb m ${param}
    </select>
</mapper>