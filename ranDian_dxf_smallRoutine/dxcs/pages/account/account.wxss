.upload{
  width: 278rpx;
  height: 190rpx;
  border: 2rpx solid #e6e6e6;
  margin: 20rpx 0 0 24rpx;
}
.upload_pic{
  width: 68rpx;
  height: 60rpx;
  display: block;
  margin: 32rpx auto 0;
}
.title{
  width: 100%;
  height: 50rpx;
  display: flex;
  justify-content: space-between;
}
.title_lable{
  font-size: 30rpx;
  height: 60rpx;
  line-height: 60rpx;
  width: 130rpx;
  margin-left: 22rpx;
  color:#888888;
}
.inputList_headBorder{
  width: 100%;
  height: 210rpx;
  display: flex;
  justify-content: space-between;
  border-bottom: 2rpx solid #ededed;
}
.headBorder{
  width: 166rpx;
  height: 166rpx;
  background: #ffffff;
  border-radius: 50%;
  margin-left: 22rpx;
  margin-top: 22rpx;
}
.head{
  width: 158rpx;
  height: 158rpx;
  display: block;
  clip-path: circle(79rpx at center); 
  margin: 4rpx auto;
}
.user_info{
  color: #353535;
  font-size: 32rpx;
  margin: auto;
  margin-left: 40rpx;
}
.user_info_blank{
  color: #b2b2b2;
  font-size: 32rpx;
  margin: auto;
  margin-left: 40rpx;
}
.right_jt{
  width: 30rpx;
  height: 30rpx;
  margin: auto;
  margin-right: 22rpx;
}
.usertype_picker{
  font-size: 32rpx;
  color: #353535;
}